{
	"name": "ContingencyTablesBayesian",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "rows",
			"type": "Variables"
		},
		{
			"name": "columns",
			"type": "Variables"
		},
		{
			"name": "counts",
			"type": "Variable"
		},
		{
			"name": "layers",
			"type": "Table",
			"template": [
				{
					"default": "Layer %1",
					"name": "name",
					"type": "String"
				},
				{
					"name": "variables",
					"type": "Variables"
				}
			]
		},
		{
			"name": "oddsRatio",
			"type": "Boolean"
		},
		{
			"name": "oddsRatioCredibleIntervalInterval",
			"type": "Number",
			"format": "%",
			"max": 0.999,
			"min": 0.0,
			"value": 0.950
		},
		{
			"name": "effectSize",
			"type": "Boolean"
		},
		{
			"name": "effectSizeCredibleIntervalInterval",
			"type": "Number",
			"format": "%",
			"max": 0.999,
			"min": 0.0,
			"value": 0.950
		},
		{
			"name": "bfIndependence",
			"type": "Boolean"
		},
		{
			"name": "samplingModel",
			"options": [
				"poisson",
				"jointMultinomial",
				"independentMultinomialRowsFixed",
				"independentMultinomialColumnsFixed",
				"hypergeometric"
			],
			"default": "independentMultinomialRowsFixed",
			"type": "List"
		},
		{
			"name": "priorConcentration",
			"type": "Number",
			"value": 1.0,
			"min": 1.0
		},
		{
			"name": "plotPosteriorOddsRatio",
			"type": "Boolean"
		},
		{
			"name": "plotPosteriorOddsRatioAdditionalInfo",
			"type": "Boolean"
		},
		{
			"name": "plotPosteriorEffectSize",
			"type": "Boolean"
		},
		{
			"name": "countsObserved",
			"type": "Boolean"
		},
		{
			"name": "countsExpected",
			"type": "Boolean"
		},
		{
			"name": "hideSmallCounts",
			"type": "Boolean"
		},
		{
			"name": "hideSmallCountsLessThan",
			"type": "Integer",
			"default": 5
		},
		{
			"name": "zTestCompareColumns",
			"type": "Boolean"
		},
		{
			"name": "zTestAdjustPValues",
			"type": "Boolean"
		},
		{
			"name": "percentagesRow",
			"type": "Boolean"
		},
		{
			"name": "percentagesColumn",
			"type": "Boolean"
		},
		{
			"name": "percentagesTotal",
			"type": "Boolean"
		},
		{
			"name": "residualsUnstandardized",
			"type": "Boolean"
		},
		{
			"name": "residualsStandardized",
			"type": "Boolean"
		},
		{
			"name": "residualsAdjustedStandardized",
			"type": "Boolean"
		},
		{
			"name": "rowOrder",
			"type": "List",
			"options": [ "ascending", "descending" ],
			"default": "ascending"
		},
		{
			"name": "plotWidth",
			"type": "Integer",
			"default": 320
		},
		{
			"name": "plotHeight",
			"type": "Integer",
			"default": 240
		},
		{
			"name": "hypothesis",
			"type": "List",
			"options": [ "groupsNotEqual", "groupOneGreater", "groupTwoGreater" ],
			"default": "groupsNotEqual"
		},
		{
			"name": "bayesFactorType",
			"type": "List",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10"
		},
		{
			"name": "columnOrder",
			"type": "List",
			"options": [ "ascending", "descending" ],
			"default": "ascending"
		}
	]
}
