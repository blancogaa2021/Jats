{
	"name": "NetworkAnalysis",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "variables",
			"type": "Variables"
		},
		{
			"name": "groupingVariable",
			"type": "Variable"
		},
		{
			"name": "colorNodesBy",
			"type": "Variable"
		},
		{
			"name": "mgmVariableType",
			"type": "Variable"
		},
		{
			"name": "estimator",
			"type": "List",
			"options": [
   				"EBICglasso",
				"pcor",
				"IsingFit",
				"IsingSampler",
				"huge",
				"adalasso",
				"mgm"
			],
			"default": "EBICglasso"
		},
		{
			"default": false,
			"name": "tableFitMeasures",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "tableCentrality",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotNetwork",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotCentrality",
			"type": "Boolean"
		},
		{
			"options": ["auto", "cor", "cov", "npn"],
			"default": "auto",
			"name": "correlationMethod",
			"type": "List"
		},
		{
			"name": "tuningParameter",
			"type": "Number",
			"value": 0.5,
			"min": 0.0,
			"max": 1.0
		},
		{
			"options": ["ebic", "ric", "stars", "cv"],
			"default": "ebic",
			"name": "criterion",
			"type": "List"
		},
		{
			"options": ["pseudoLikelihood", "univariateRegressions", "bivariateRegressions", "logLinear"],
			"default": "pseudoLikelihood",
			"name": "isingEstimator",
			"type": "List"
		},
		{
			"name": "nFolds",
			"type": "Integer",
			"min": 1,
			"max": 10000,
			"default": 1
		},
		{
			"options": ["mean", "median"],
			"default": "mean",
			"name": "split",
			"type": "List"
		},
		{
			"options": ["and", "or"],
			"default": "and",
			"name": "rule",
			"type": "List"
		},
		{
			"options": ["pairwise", "listwise"],
			"default": "pairwise",
			"name": "missingValues",
			"type": "List"
		},
		{
			"default": true,
			"name": "weightedNetwork",
			"type": "Boolean"
		},
		{
			"default": true,
			"name": "signedNetwork",
			"type": "Boolean"
		},
		{
			"options": ["maximum", "minimim"],
			"default": "maximum",
			"name": "sampleSize",
			"type": "List"
		},
		{
			"name": "normalizeCentrality",
			"options": ["normalized", "raw", "relative"],
			"default": "normalized",
			"type": "List"
		},
		{
			"options": ["spring", "circle"],
			"default": "spring",
			"name": "layout",
			"type": "List"
		},
		{
			"name": "repulsion",
			"type": "Number",
			"value": 1.0,
			"min": 0.0,
			"max": 10.0
		},
		{
			"name": "edgeSize",
			"type": "Number",
			"value": 1.0
		},
		{
			"name": "nodeSize",
			"type": "Number",
			"value": 1.0,
			"min": 0.0,
			"max": 10.0
		},
		{
			"name": "maxEdgeStrength",
			"type": "Number",
			"value": 0.0,
			"min": 0.0,
			"max": 10.0
		},
		{
			"name": "minEdgeStrength",
			"type": "Number",
			"value": 0.0,
			"min": 0.0,
			"max": 10.0
		},
		{
			"name": "cut",
			"type": "Number",
			"value": 0.0,
			"min": 0.0,
			"max": 10.0
		},
		{
			"default": true,
			"name": "showDetails",
			"type": "Boolean"
		},
		{
			"name": "showVariableNames",
			"type": "List",
			"options": [
				"In legend",
				"In nodes"
			],
			"default": "In nodes"
		},
		{
			"name": "showLegend",
			"type": "List",
			"options": [
				"No legend",
				"All plots",
				"In plot number: "
			],
			"default": "All plots"
		},
		{
			"name": "legendNumber",
			"type": "Integer",
			"default": 1,
			"min": 0
		},
		{
			"name": "nodeColors",
			"type": "List",
			"options": [
   				"rainbow",
				"colorblind",
				"pastel",
				"gray",
				"R",
				"ggplot2"
			],
			"default": "rainbow"
		},
		{
			"name": "edgeColors",
			"type": "List",
			"options": [
   				"classic",
				"colorblind",
				"gray",
				"Hollywood",
				"Borkulo",
				"TeamFortress",
				"Reddit",
				"Fried"
			],
			"default": "classic"
		},
		{
			"name": "showMgmVariableType",
			"type": "List",
			"options": ["mgmNoShow", "mgmNodeColor", "mgmNodeShape"],
			"default": "mgmNodeShape"
		},
		{
			"name": "graphSize",
			"type": "List",
			"options": ["graphSizeFixed", "graphSizeFree"],
			"default": "graphSizeFixed"
		},
		{
			"name": "plotWidthNetwork",
			"type": "Integer",
			"default": 480
		},
		{
			"name": "plotHeightNetwork",
			"type": "Integer",
			"default": 320
		},
		{
			"name": "plotWidthCentrality",
			"type": "Integer",
			"default": 480
		},
		{
			"name": "plotHeightCentrality",
			"type": "Integer",
			"default": 320
		}
	]
}
