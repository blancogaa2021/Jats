{
	"name": "ABTestBayesian",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "n1",
			"type": "Variable"
		},
		{
			"name": "y1",
			"type": "Variable"
		},
		{
			"name": "n2",
			"type": "Variable"
		},
		{
			"name": "y2",
			"type": "Variable"
		},
		{
			"format": "",
			"name": "normal_mu",
			"type": "Number",
			"value": 0
		},
		{
			"format": "",
			"name": "normal_sigma",
			"type": "Number",
			"value": 1
		},
		{
			"name": "bayesFactorType",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10",
			"type": "List"
		},
		{
			"default": false,
			"name": "plotPriorAndPosterior",
			"type": "Boolean"
		},
		{
			"name": "plotPosteriorType",
			"type": "List",
			"options": [
				"LogOddsRatio",
				"OddsRatio",
				"RelativeRisk",
				"AbsoluteRisk",
				"p1&p2"
			],
			"default": "LogOddsRatio"
		},
		{
			"default": false,
			"name": "plotSequentialAnalysis",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotPriorOnly",
			"type": "Boolean"
		},
		{
			"name": "plotPriorType",
			"type": "List",
			"options": [
				"LogOddsRatio",
				"OddsRatio",
				"RelativeRisk",
				"AbsoluteRisk",
				"p1&p2",
				"p1",
				"p2"
			],
			"default": "LogOddsRatio"
		},
		{
			"name": "orEqualTo1Prob",
			"type": "Number",
			"value": 0.5,
			"min": 0.0,
			"max": 1.0
		},
		{
			"name": "orGreaterThan1Prob",
			"type": "Number",
			"value": 0.25,
			"min": 0.0,
			"max": 1.0
		},
		{
			"name": "orLessThan1Prob",
			"type": "Number",
			"value": 0.25,
			"min": 0.0,
			"max": 1.0
		},
		{
			"name": "numSamples",
			"type": "Integer",
			"default": 10000,
			"min": 1
		},
		{
			"name": "descriptives",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "bayesFactorOrder",
			"options": [ "bestModelTop", "nullModelTop" ],
			"default": "nullModelTop",
			"type": "List"
		}
	]
}
