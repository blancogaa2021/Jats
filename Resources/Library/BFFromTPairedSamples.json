{
	"name": "BFFromTPairedSamples",
	"autorun": true,
	"version": "1.0",
	"options": [
		{
			"format": "",
			"name": "tStatistic",
			"type": "Number"
		},
		{
			"format": "",
			"name": "n1Size",
			"type": "Number"
		},
		{
			"format": "",
			"name": "n2Size",
			"type": "Number"
		},		
		{
			"name": "hypothesis",
			"options": [ "groupsNotEqual", "groupOneGreater", "groupTwoGreater" ],
			"default": "groupsNotEqual",
			"type": "List"
		},
		{
			"format": "",
			"name": "priorWidth",
			"type": "Number",
			"value": 0.7070,
			"min": 0,
			"max": 2
		},
		{
			"name": "bayesFactorType",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10",
			"type": "List"
		},
		{
			"default": false,
			"name": "errorEstimate",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotPriorAndPosterior",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotPriorAndPosteriorAdditionalInfo",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotBayesFactorRobustness",
			"type": "Boolean"
		}
	]
}
