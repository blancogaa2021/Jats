{
	"name": "BASRegressionLinearLink",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "dependent",
			"type": "Variable"
		},
		{
			"name": "betaBinomialParamA",
			"type": "Number",
			"value": 1.000,
			"min": 0
		},
		{
			"name": "betaBinomialParamB",
			"type": "Number",
			"value": 1.000,
			"min": 0
		},
		{
			"name": "covariates",
			"type": "Variables"
		},
		{
			"default": false,
			"name": "plotLogPosteriorOdds",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotCoefficientsPosterior",
			"type": "Boolean"
		},
		{
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10",
			"name": "bayesFactorType",
			"type": "List"
		},
		{
			"options": ["AIC", "BIC", "EB-global", "EB-local", "g-prior", "hyper-g", "hyper-g-laplace", "hyper-g-n", "ZS-full", "ZS-null"],
			"default": "BIC",
			"name": "priorRegressionCoefficients",
			"type": "List"
		},
		{
			"options": ["BAS", "MCMC"],
			"default": "BAS",
			"name": "samplingMethod",
			"type": "List"
		},
		{
			"name": "numberOfModels",
			"type": "Number"
		},
		{
			"name": "iterationsMCMC",
			"type": "Integer",
			"default": 0
		},
		{
			"options": ["beta.binomial", "uniform"],
			"default": "uniform",
			"name": "modelPrior",
			"type": "List"
		},
		{
			"name": "priorCovariates",
			"type": "Number",
			"value": 0.354,
			"min": 0,
			"max": 2
		},
		{
			"name": "gPriorParameter",
			"type": "Number",
			"value": 3,
		}
	]
}
