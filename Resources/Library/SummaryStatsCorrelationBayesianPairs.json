{
	"name": "SummaryStatsCorrelationBayesianPairs",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"format": "",
			"name": "sampleSize",
			"type": "Integer",
			"min": 2
		},
		{
			"format": "",
			"name": "pearsonsR",
			"type": "Number",
			"min": -1,
			"max": 1
		},
		{
			"name": "hypothesis",
			"options": [ "correlated", "correlatedPositively", "correlatedNegatively" ],
			"default": "correlated",
			"type": "List"
		},
		{
			"format": "",
			"name": "priorWidth",
			"type": "Number",
			"value": 1.0,
			"min" : 0,
			"max" : 2.0
		},
		{
			"default": false,
			"name": "plotPriorAndPosterior",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotPriorAndPosteriorAdditionalInfo",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotBayesFactorRobustness",
			"type": "Boolean"
		},
		{
			"name": "bayesFactorType",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10",
			"type": "List"
		}
	]
}
