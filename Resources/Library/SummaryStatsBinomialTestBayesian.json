{
	"name": "SummaryStatsBinomialTestBayesian",
	"autorun": true,
	"version": "1.0",
	"options": [
		{
			"name": "hypothesis",
			"options": [ "notEqualToTestValue", "greaterThanTestValue", "lessThanTestValue" ],
			"default": "notEqualToTestValue",
			"type": "List"
		},
		{
			"format": "",
			"name": "testValue",
			"type": "Number",
			"value": 0.5,
			"min": 0,
			"max": 1
		},
		{
			"name": "bayesFactorType",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10",
			"type": "List"
		},
		{
			"default": false,
			"name": "plotPriorAndPosterior",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotPriorAndPosteriorAdditionalInfo",
			"type": "Boolean"
		},
		{
			"format": "",
			"name": "successes",
			"type": "Integer",
			"min": 0
		},
		{
			"format": "",
			"name": "failures",
			"type": "Integer",
			"min": 0
		},
		{
			"format": "",
			"name": "betaPriorParamA",
			"type": "Number",
			"value": 1,
			"min": 0
		},
		{
			"format": "",
			"name": "betaPriorParamB",
			"type": "Number",
			"value": 1,
			"min": 0
		}
	]	
}
