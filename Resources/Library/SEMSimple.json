{
	"name": "SEMSimple",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "model",
			"type": "String"
		},
		{
			"name": "modelName",
			"type": "List",
			"options": [
   				"Model 1",
				"Model 2",
				"Model 3",
				"Model 4",
				"Model 5",
				"Model 6",
				"Model 7",
				"Model 8",
				"Model 9",
				"Model 10"
			]
		},
		{
			"name": "errorCalculation",
			"type": "List",
			"options": [
				"standard",
				"robust",
				"bootstrap"
			],
			"default": "standard"
		},
		{
			"name": "errorCalculationBootstrapSamples",
			"type": "Integer",
			"default": 1000,
			"min": 0
		},
		{
			"name": "outputMardiasCoefficients",
			"type": "Boolean"
		},
		{
			"name": "outputAdditionalFitMeasures",
			"type": "Boolean"
		},
		{
			"name": "outputFittedCovarianceCorrelations",
			"type": "Boolean"
		},
		{
			"name": "outputResidualCovarianceCorrelations",
			"type": "Boolean"
		},
		{
			"name": "outputObservedCovarianceCorrelations",
			"type": "Boolean"
		},
		{
			"name": "outputModificationIndices",
			"type": "Boolean"
		},
		{
			"name": "outputModificationIndicesHideLowIndices",
			"type": "Boolean"
		},
		{
			"name": "outputModificationIndicesHideLowIndicesThreshold",
			"type": "Integer",
			"default": 10
		},
		{
			"name": "outputObservedCovarianceCorrelations",
			"type": "Boolean"
		},
		{
			"name": "groupingVariable",
			"type": "Variable"
		},
		{
			"name": "estimator",
			"type": "List",
			"options": [
				"automatic",
				"ML",
				"GLS",
				"WLS",
				"ULS",
				"DWLS"
			],
			"default": "automatic"
		},
		{
			"name": "includeMeanStructure",
			"type": "Boolean"
		},
		{
			"name": "assumeFactorsUncorrelated",
			"type": "Boolean"
		},
		{
			"name": "fixExogenousCovariates",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "factorStandardisation",
			"type": "List",
			"options": [
   				"factorLoadings",
				"residualVariance",
				"none"
			]
		},
		{
			"name": "fixManifestInterceptsToZero",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "fixLatentInterceptsToZero",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "omitResidualSingleIndicator",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "residualVariances",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "correlateExogenousLatents",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "addThresholds",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "addScalingParameters",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "correlateDependentVariables",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "emulation",
			"type": "List",
			"options": [
				"none",
				"Mplus",
				"EQS"
			],
			"default": "none"
        },
        {
            "name": "eq_loadings",
            "type": "Boolean",
            "default": false
        },
        {
            "name": "eq_intercepts",
            "type": "Boolean",
            "default": false
        },
        {
            "name": "eq_means",
            "type": "Boolean",
            "default": false
        },
        {
            "name": "eq_thresholds",
            "type": "Boolean",
            "default": false
        },
        {
            "name": "eq_regressions",
            "type": "Boolean",
            "default": false
        },
        {
            "name": "eq_residuals",
            "type": "Boolean",
            "default": false
        },
        {
            "name": "eq_residualcovariances",
            "type": "Boolean",
            "default": false
        },
        {
            "name": "eq_variances",
            "type": "Boolean",
            "default": false
        },
        {
            "name": "eq_lvcovariances",
            "type": "Boolean",
            "default": false
        },
		{
			"name": "Data",
			"type": "List",
			"options": [
				"raw",
				"varcov"
			],
			"default": "raw"
        },
		{
			"name": "SampleSize",
			"type": "Integer",
			"default": 0,
			"min": 0
		}
	]
}
