{
	"name": "ReliabilityAnalysis",
	"autorun": true,
	"version": "1.00",
	"options": [
    {
      "name": "variables",
      "type": "Variables"
    },
    {
    	"name": "alphaScale",
    	"type": "Boolean",
        "default" : false
    },
    {
        "name": "alphaScaleStandardized",
        "options": ["_1unstandardized", "_2standardized"],
        "default": "_1unstandardized",
        "type": "List"
    },
    {
    	"name": "gutmannScale",
    	"type": "Boolean",
			"default" : false
    },
    {
    	"name": "averageInterItemCor",
    	"type": "Boolean",
			"default" : false
    },
    {
    	"name": "mcDonaldScale",
    	"type": "Boolean",
			"default" : true
    },
    {
    	"name": "glbScale",
    	"type": "Boolean",
			"default" : false
    },
    {
    	"name": "meanScale",
    	"type": "Boolean",
			"default" : false
    },
    {
    	"name": "sdScale",
    	"type": "Boolean",
			"default" : false
    },
    {
    	"name": "alphaItem",
    	"type": "Boolean",
			"default" : false
    },
    {
    	"name": "gutmannItem",
    	"type": "Boolean",
			"default" : false
    },
    {
    	"name": "mcDonaldItem",
    	"type": "Boolean",
			"default" : false
    },
    {
    	"name": "itemRestCor",
    	"type": "Boolean",
			"default" : false
    },
    {
    	"name": "meanItem",
    	"type": "Boolean",
			"default" : false
    },
    {
    	"name": "sdItem",
    	"type": "Boolean",
			"default" : false
    },
    {
      "name": "reverseScaledItems",
      "type": "Variables"
    },
    {
      "name": "missingValues",
      "options": ["excludeCasesListwise", "excludeCasesPairwise"],
      "default": "excludeCasesListwise",
      "type": "List"
    },
		{
    	"name": "confAlpha",
    	"type": "Boolean",
			"default" : false
    },
		{
    	"name": "confAlphaLevel",
			"type": "Number",
    	"format": "%",
			"max": 0.999,
			"min": 0.000,
			"value": 0.950
    }
	]
}
