{
	"name": "PrincipalComponentAnalysis",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "variables",
			"type": "Variables"
		},
        {
            "name": "factorMethod",
            "options": [ "eigenValues", "manual", "parallelAnalysis" ],
            "default": "parallelAnalysis",
            "type": "List"
        },
        {
            "name": "eigenValuesBox",
            "type": "Number",
            "min": 0.0,
            "value": 1.0
        },
        {
            "name": "numberOfFactors",
            "type": "Integer",
            "default": 1,
						"min": 1
        },
        {
            "name": "rotationMethod",
            "options": [ "orthogonal", "oblique" ],
            "default": "oblique",
            "type": "List"
        },
        {
            "name": "orthogonalSelector",
            "options": ["none", "varimax", "quartimax","bentlerT","equamax","varimin"],
            "type": "List"
        },
        {
            "name": "obliqueSelector",
            "options": [ "promax", "oblimin", "simplimax", "bentlerQ", "biquartimin", "cluster" ],
            "type": "List"
        },
        {
        	"name": "highlightSlider",
        	"min": 0,
        	"max": 1,
        	"type": "Number",
        	"value": 0.4
        },
        {
        	"name": "highlightText",
        	"min": 0,
        	"max": 1,
        	"type": "Number",
        	"value": 0.4
        },
    	{
			"name": "incl_loadings",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "incl_correlations",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "incl_GoF",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "incl_fitIndices",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "incl_pathDiagram",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "plotWidthPathDiagram",
			"type": "Integer",
			"default": 480
		},
		{
			"name": "plotHeightPathDiagram",
			"type": "Integer",
			"default": 0
		},      
		{
			"name": "incl_screePlot",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "plotWidthScreePlot",
			"type": "Integer",
			"default": 300
		},
		{
			"name": "plotHeightScreePlot",
			"type": "Integer",
			"default": 300
		}
	]
}
