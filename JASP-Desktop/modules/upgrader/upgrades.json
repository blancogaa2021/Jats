	{
		"msg": 	"For version 0.12 we merged Bayesian Correlation Pairs into Bayesian Correlation Matrix, all options are migrated and the values kept.",
		"from": { "module": "Regression",		"version": "0.11.1", 	"function": "CorrelationBayesianPairs"	},
		"to":	{ "module": "Regression",		"version": "0.12", 		"function": "CorrelationBayesian"		},

		"options":
		[
			{
				"changes": 
				{
					"pearson": 			{ "_boolOp": "NOT", "_args": [ { "corcoefficient": "Kendall" } ], "comment": "This can really only be Pearson but we do it as a NOT-kendall to make sure pearson also gets set as default == true in case something weird happened to the options in the file." },
					"spearman":			false,
					"kendall":			{ "_args": [ { "corcoefficient": "Kendall" } ] }
				}
			},
			{
			"changes":
			{ 
				"ci":						{ "_renamed":	"credibleInterval" 	},
				"kappa":					{ "_renamed":	"priorWidth" 			},
				"variables":				{ "_copied":	"pairs" 				},
				"corcoefficient": 			null,
				"plotBfRobustness":			{ "_renamed": "plotBayesFactorRobustness" },
				"plotBfRobustnessAddInfo":	{ "_renamed": "plotBayesFactorRobustnessAdditionalInfo" },
				"plotBfSequential":			{ "_renamed": "plotSequentialAnalysis" },
				"plotBfSequentialAddInfo":	{ "_renamed": "plotSequentialAnalysisRobustness" },
				"displayPairwise":			true,
				"reportBayesFactors":		true
			}
		},
		{
			"changes": {
				"variables": { "_modified": "Flatten", "comment": "We've copied pairs to here, but because it expects columns instead of pairs we 'Flatten' it." } 
			}
		},
		{
			"conditional": 	{ "_args": [ { 			"hypothesis": "correlated" } ] },
			"changes": 		{ "hypothesis":	null,	"alternative": "two.sided"	 }
		},
		{
			"conditional": 	{ "_args": [ { 			"hypothesis": "correlatedPositively" } ] },
			"changes": 		{ "hypothesis":	null,	"alternative": "greater"	 }
		},
		{
			"conditional": 	{ "_args": [ { 			"hypothesis": "correlatedNegatively" } ] },
			"changes": 		{ "hypothesis":	null,	"alternative": "less"	 }
		},

		{
			"conditional": 	{ "_args": [ {	"missingValues": "excludeAnalysisByAnalysis" } ] },
			"changes": 		{ 				"missingValues": "excludePairwise"	 }
		},
		{
			"changes": {
				"plotPriorPosterior": 					{ "_renamed": 	"plotPriorAndPosterior" },
				"plotPriorPosteriorAddEstimationInfo":	{ "_args":	[{	"plotPriorAndPosteriorAdditionalInfo": true }] },
				"plotPriorPosteriorAddTestingInfo":		{ "_args":	[{	"plotPriorAndPosteriorAdditionalInfo": true }] }
			}
		},
		{
			"comment": "The previous change that needed info from plotPriorAndPosteriorAdditionalInfo is now processed so we can remove it from the options", 
			"changes": { "plotPriorAndPosteriorAdditionalInfo": null }
		}
	]
	},

